import multiprocessing
import queue

class Neora:

    def __init__(self, messages=queue.Queue()):
        self.messages = messages
        self.__executed()

    def __task(self):
        print(self.messages.get())

    def __executed(self):
        for i in range(10):
            if self.messages.empty() == False:
                p = multiprocessing.Process(target=self.__task())
                p.start()


    def add_to_queue(self, message):
        for mes in message.queue:
            self.messages.put(mes)
        self.__executed()


if __name__ == '__main__':
        queue = queue.Queue()
        queue.put('b')
        queue.put('b')
        queue.put('b')
        c = Neora(queue)


        c.add_to_queue(queue)
